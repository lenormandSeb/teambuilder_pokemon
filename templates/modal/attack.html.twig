<hr>
    <input type="text" id="myInput" class="form-control" onkeyup="myFunction()" placeholder="Search for names..">
<hr>
<div>
    <table class="table">
        <tbody id="myULA">
            <tr>
            {% for attaque in attaques %}
                <td>{{ attaque.name }}</td>
                <td><img src="{{ asset('categorie/Miniature_Categorie_'~ attaque.categorie ~'_LGPE.png') }}" style="heigth: 15px; width: 68px"></td>
                <td><img src="{{ asset('type/Miniature_Type_'~ attaque.getTypeAttack.name ~'_EB.png') }}" style="heigth: 15px; width: 68px"></td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
</div>
<script>
$(document).ready(function() {
    $('.submitter').click(function(){
        url = $(this).attr('data-target');
        $.ajax({
            type: 'get',
            url: url,
            success: function(data){
                document.location.href = "{{ path('app_team_coverage') }}"
            }
        })
    })
})
function myFunction() {
  // Declare variables
  var input, filter, ul, li, a, i, txtValue;
  input = document.getElementById('myInput');
  filter = input.value.toUpperCase();
  ul = document.getElementById("myULA");
  li = ul.getElementsByTagName('tr');

  // Loop through all list items, and hide those who don't match the search query
  for (i = 0; i < li.length; i++) {
    a = li[i].getElementsByTagName("td")[0];
    txtValue = a.textContent || a.innerText;
    if (txtValue.toUpperCase().indexOf(filter) > -1) {
      li[i].style.display = "";
    } else {
      li[i].style.display = "none";
    }
  }
}
</script>